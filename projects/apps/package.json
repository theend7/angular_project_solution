{
    "name": "apps",
    "version": "0.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "clib-common": "ng build lib-common --configuration production",
        "wclib-common": "wait-on dist/lib-common/public-api.d.ts",
        "cwlib-common": "ng build lib-common --watch",
        "compile-app-1": "npm-run-all clib-common capp-1",
        "capp-1": "ng build app-1 --configuration production",
        "sapp-1": "ng serve -o --port 4201",
        "app-1": "npm run clean && npm-run-all --parallel cwlib-common wclib-common capp-1 sapp-1"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.2.0",
        "@angular/common": "^15.2.0",
        "@angular/compiler": "^15.2.0",
        "@angular/core": "^15.2.0",
        "@angular/forms": "^15.2.0",
        "@angular/platform-browser": "^15.2.0",
        "@angular/platform-browser-dynamic": "^15.2.0",
        "@angular/router": "^15.2.0",
        "eslint": "^8.33.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.2.10",
        "@angular/cli": "~15.2.10",
        "@angular/compiler-cli": "^15.2.0",
        "@types/jasmine": "~4.3.0",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "ng-packagr": "^15.2.2",
        "npm-run-all": "^4.1.5",
        "typescript": "~4.9.4",
        "wait-on": "^7.2.0"
    }
}
